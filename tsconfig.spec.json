/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist/out-tsc-server",
    "types": [
      "node",
      "@angular/localize"
    ]
  },
  "files": [
    "src/main.ts",
    "src/main.server.ts",
    "server.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts",
    "src/**/*.ts"
  ]
}
